<Window x:Class="SMS.Common.AuthenticateAdmin.AuthenticateAdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SMS.Common.AuthenticateAdmin"
        mc:Ignorable="d"
        Title="AuthenticateAdminWindow" Height="300" Width="300">
    <Grid>
        <GroupBox Header="Scenario Admin Password"
                  Margin="0,0,0,0"
                  HorizontalAlignment="Center"
                  VerticalAlignment="Center">
            <StackPanel Orientation="Horizontal"
                        Margin="25">
                <PasswordBox Name="pass_textbox"
                             Width="200"
                             Height="35"
                             FontSize="20" />
            </StackPanel>
        </GroupBox>
    </Grid>
</Window>
