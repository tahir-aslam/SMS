<Window x:Class="SMS.AdminPanel.AdminPasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Admin Password Window" MaxHeight="165" MaxWidth="290" KeyDown="Window_KeyDown" WindowStyle="ToolWindow"  WindowStartupLocation="CenterScreen">
    <Grid>
        <GroupBox Header="Scenario Admin Password" Margin="0,0,0,0" HorizontalAlignment="Center" VerticalAlignment="Center">
            <StackPanel Orientation="Horizontal" Margin="25">
                <PasswordBox Name="pass_textbox" Width="200" Height="35" FontSize="20"/>
            </StackPanel>
        </GroupBox>
    </Grid>
</Window>
